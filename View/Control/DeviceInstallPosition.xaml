<UserControl x:Class="project.wpf.f.icooling._2002.View.Control.DeviceInstallPosition"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:project.wpf.f.icooling._2002.View.Control"
			 xmlns:pu="clr-namespace:Panuon.UI.Silver;assembly=Panuon.UI.Silver"
             mc:Ignorable="d"
             d:DesignHeight="70" d:DesignWidth="500">
	<Grid>
		<GroupBox
				Header="3、安装位置"
				pu:GroupBoxHelper.CornerRadius="1"
				pu:GroupBoxHelper.IsSplitLineVisible="True">
			<ComboBox Margin="0 0 30 0"
					  pu:ComboBoxHelper.Header=" "
					  Name="ComboBox"
					  SelectionChanged="McmbOption_SelectionChanged"
					  DisplayMemberPath="Description"
					  SelectedValuePath="Id"
					  ItemsSource="{Binding}"
					  ></ComboBox>
		</GroupBox>
	</Grid>
</UserControl>